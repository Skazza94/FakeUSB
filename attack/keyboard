DELAY(81)			2000
PRESS_KEYS(81) 		CTRL+ALT+t
DELAY(81) 			2000
WRITE(81) 			"export "
PRESS_KEYS(81) 		CAPSL
WRITE(81) 			"env_var_1=/"
PRESS_KEYS(81) 		CAPSL
WRITE(81) 			"dev/"
PRESS_KEYS(81) 		ENTER
WRITE(81) 			"echo $"
PRESS_KEYS(81) 		CAPSL
WRITE(81) 			"env_var_1"
PRESS_KEYS(81) 		CAPSL
PRESS_KEYS(81) 		ENTER
DELAY(81) 			10000
PRESS_KEYS(81) 		ALT+F4